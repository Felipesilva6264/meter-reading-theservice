{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./",
      "typeRoots": [
        "./node_modules/@types",
        "./src/types" 
      ],
      "outDir": "./dist" // Adicione esta linha para definir o diretório de saída
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }
  